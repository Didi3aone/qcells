.main-slider
{
    margin-bottom: 14px;
    overflow: hidden;
}

.main-slider .mainslide
{
    overflow: hidden;
    width: 100%;
    max-width: 1148px;
    height: 100%;
    max-height: 542px;
}

.hotslider .mainslide
{
    overflow: hidden;
    width: 100%;
    max-width: 1148px;
    height: 100%;
    max-height: 340px;
}

.slide > * {
    max-width: 100%;
    max-height: 542px;
}

.hotslider .slide > * {
    max-width: 100%;
    max-height: 340px;
}

.slider
{
    position: relative;
}

.slider .slide-prev , .slider .slide-next{
    cursor: pointer;
    height: 100%;
    width: 50px;
    position: absolute;
    top: 0px;
    background: url('../img/ui/sllarrow.png') center center no-repeat, url('../img/ui/gradleft.png') top left repeat-y;
    padding: 0px;
	opacity: 0.5;
    -webkit-transition: opacity 0.3s ease-out 0s;
    -moz-transition: opacity 0.3s ease-out 0s;
    -o-transition: opacity 0.3s ease-out 0s;
    transition: opacity 0.3s ease-out 0s;
}
.slider .slide-next{
    background:  url('../img/ui/slrarrow.png') center center no-repeat, url('../img/ui/gradright.png') top right repeat-y;
}
.slider .slide-prev  {
	left: 0;
}
.slider .slide-next  {
	right: 0;
}
.slider .slide-prev:before, .slider .slide-next:before {
	display: block;
	width: 50px;
	height: 50px;
	font-size: 40px;
	line-height: 50px;
	text-align: center;
    position: absolute;
    top: 50%;
    margin-top: -25px;
    color: #FFFFFF;
}
.slider .slide-prev:before {
}
.slider .slide-next:before {
}
.slider .slide-next:hover, .slider .slide-prev:hover {
    opacity: 0.8;
}
.es-caption {
    position: absolute;
