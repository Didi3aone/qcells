.scrollable {
  position: relative;
}

.scrollable:focus {
  outline: 0;
}

.scrollable .viewport {
  position: relative;
  overflow: hidden;
}

.scrollable .viewport .overview {
  position: absolute;
}

.scrollable .scroll-bar {
  display: none;
}

.scrollable .scroll-bar.vertical {
  position: absolute;
  right: 0;
  height: 100%;
}

.scrollable .scroll-bar.horizontal {
  position: relative;
  width: 100%;
}

.scrollable .scroll-bar .thumb {
  position: absolute;
}

.scrollable .scroll-bar.vertical .thumb {
  width: 100%;
  min-height: 10px;
}

.scrollable .scroll-bar.horizontal .thumb {
  height: 100%;
  min-width: 10px;
  left: 0;
}

.not-selectable {
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}

.scrollable.avian-scroll {
  padding-right: 17px;
}

.scrollable.avian-scroll .scroll-bar {
  background-color: #C4E2BF;
}

.scrollable.avian-scroll .scroll-bar .thumb {
  background-color: #006C67;
}

.scrollable.avian-scroll .scroll-bar:hover .thumb {
  background-color: #107C77;
}

.scrollable.avian-scroll .scroll-bar.vertical {
  width: 10px;
}

.scrollable.avian-scroll .scroll-bar.horizontal {
  height: 10px;
  margin-top: 2px;
}

.scrollable.avian-scroll.avian-scroll-yonly .scroll-bar.horizontal {
  height: 0px;
    display: none;
}

