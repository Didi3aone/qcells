/* `````````````````````````````````````````````````````````` MARKMARK GLOBAL(css resets) `````````````````````````````````````````````````````````` */


*
{
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    margin: 0px;
}

html
{
    transform-origin: left top;
    -moz-transform-origin: left top;
    -webkit-transform-origin: left top;
    -ms-transform-origin: left top;
    -o-transform-origin: left top;
    background-color: #FFFFFF;
    width: 100%;
}

body
{
    font-family: 'Raleway', sans-serif;
    font-family: 'Roboto', sans-serif;
    font-size: 18px;
}

h1,h2,h3,h4,h5,h6,p {
 line-height: 1.45;
}
h1 { font-size: 4em; }
h2 { font-size: 3em; }
h3 { font-size: 2em; }

::-webkit-input-placeholder {color: #8E9092;}
:-moz-placeholder { /* Firefox 18- */color: #8E9092;}
::-moz-placeholder {  /* Firefox 19+ */color: #8E9092;}
:-ms-input-placeholder {color: #8E9092;}

.hidtxt
{
    display: none;
    width: 0px;
    top: 0px;
    overflow: hidden;
    font-size: 0px;
    line-height: 0px;
    text-indent: -200%;
    white-space: nowrap;
    margin: 0px;
    padding: 0px;
}

.lead
{
    clear: both;
    width: 100%;
}

.section-nocrumb
{
    margin-top: 55px;
}

.section
{
    width: 100%;
    margin-bottom: 80px;
}

.section-smargin
{
    margin-bottom: 30px;
}

.section-smargin .superbig-btn
{
    margin-top: 30px;
    margin-bottom: 30px;
}

.section .section_content
{
    max-width: 1148px;
    width: 100%;
    margin: 0px auto;
    padding: 0px 5px;
}


